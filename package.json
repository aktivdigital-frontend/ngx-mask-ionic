{
	"name": "ngx-mask-ionic",
	"version": "1.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"lib:build": "ng build ngx-mask-ionic-lib",
		"lib:build-watch": "ng build ngx-mask-ionic-lib --watch",
		"lib:copy-readme": "node_modules/.bin/copyfiles ./README.md ./dist/ngx-mask-ionic-lib",
		"lib:publish": "npm run lib:build && npm run lib:copy-readme && cd dist/ngx-mask-ionic-lib && npm publish",
		"lint-fix": "node_modules/.bin/tslint --fix --project tsconfig.json",
		"format-ts": "node_modules/.bin/prettier --write --list-different \"src/**/*.ts\"",
		"format-html": "node_modules/.bin/glob-run html-beautify -r \"src/**/*.html\"",
		"format-css": "node_modules/.bin/glob-run css-beautify -r \"src/**/*.scss\"",
		"lib:format-ts": "node_modules/.bin/prettier --write --list-different \"projects/**/*.ts\"",
		"lib:format-html": "node_modules/.bin/glob-run html-beautify -r \"projects/**/*.html\"",
		"lib:format-css": "node_modules/.bin/glob-run css-beautify -r \"projects/**/*.scss\"",
		"lint-format": "npm run lint-fix && npm run format-ts && npm run format-html && npm run format-css && npm run lib:format-ts && npm run lib:format-html && npm run lib:format-css",
		"check-for-tslint-prettier-conflicts": "node_modules/.bin/tslint-config-prettier-check ./tslint.json"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~8.2.14",
		"@angular/common": "~8.2.14",
		"@angular/compiler": "~8.2.14",
		"@angular/core": "~8.2.14",
		"@angular/forms": "~8.2.14",
		"@angular/platform-browser": "~8.2.14",
		"@angular/platform-browser-dynamic": "~8.2.14",
		"@angular/router": "~8.2.14",
		"core-js": "^3.6.5",
		"rxjs": "~6.4.0",
		"zone.js": "~0.9.1"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.803.29",
		"@angular-devkit/build-ng-packagr": "~0.803.29",
		"@angular/cli": "~8.3.29",
		"@angular/compiler-cli": "~8.2.14",
		"@angular/language-service": "~8.2.14",
		"@types/jasmine": "~3.3.8",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "~8.9.4",
		"codelyzer": "^5.0.1",
		"copyfiles": "^2.1.0",
		"glob-run": "^0.1.6",
		"jasmine-core": "^3.99.1",
		"jasmine-spec-reporter": "~4.2.1",
		"js-beautify": "^1.8.8",
		"karma": "~4.1.0",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~2.0.1",
		"karma-jasmine": "~2.0.1",
		"karma-jasmine-html-reporter": "^1.4.0",
		"ng-packagr": "^5.7.1",
		"prettier": "^1.15.3",
		"protractor": "~5.4.0",
		"ts-node": "~7.0.0",
		"tsickle": "~0.36.0",
		"tslib": "^1.9.0",
		"tslint": "~5.15.0",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "~3.4.1",
		"sass": "^1.32.0",
		"sass-loader": "^8.0.2"
	},
	"overrides": {
		"stylus": "https://github.com/stylus/stylus/archive/refs/tags/0.54.5.tar.gz",
		"@angular-devkit/build-angular": {
			"stylus": "https://github.com/stylus/stylus/archive/refs/tags/0.54.5.tar.gz"
		}
	}
}
